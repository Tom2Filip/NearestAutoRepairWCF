<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="username" value="test" />
    <add key="password" value="123456" />
   </appSettings>
  
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_INearestAutoRepairService" messageEncoding="Mtom">
                    <security mode="Transport">
                        <transport clientCredentialType="Basic" />
                    </security>
                </binding>
            </basicHttpBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_INearestAutoRepairService">
                    <security mode="TransportWithMessageCredential">
                        <transport clientCredentialType="None" />
                        <message clientCredentialType="UserName" />
                    </security>
                </binding>
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="https://tompc/NearestAutoRepairPublicIISHost/NearestAutoRepairServicePublic.svc"
                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_INearestAutoRepairService"
                contract="NearestServicePublicRef.INearestAutoRepairService"
                name="WSHttpBinding_INearestAutoRepairService" />
            <endpoint address="https://tompc/NearestAutoRepairIISHost/NearestAutoRepairService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_INearestAutoRepairService"
                contract="NearestAutoRepairServiceRef3.INearestAutoRepairService"
                name="BasicHttpBinding_INearestAutoRepairService" />
        </client>
    </system.serviceModel>
